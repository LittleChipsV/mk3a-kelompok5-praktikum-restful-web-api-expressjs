{
  "name": "praktikum-restfulapi-crud-operations-expressjs-sequelize",
  "version": "1.0.0",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:create": "npx sequelize-cli db:create --config src/config/database.js",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/database.js --migrations-path src/migrations",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config src/config/database.js --migrations-path src/migrations",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --config src/config/database.js --migrations-path src/migrations",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/database.js --seeders-path src/seeders",
    "db:seed:undo": "npx sequelize-cli db:seed:undo --config src/config/database.js --seeders-path src/seeders",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all --config src/config/database.js --seeders-path src/seeders"
  },
  "keywords": [],
  "author": "Kelompok 5 - XII PPLG 1",
  "repository": {
    "type": "git",
    "url": "https://github.com/LittleChipsV/mk3a-kelompok5-praktikum-restful-web-api-expressjs.git"
  },
  "license": "ISC",
  "description": "Proyek mata pelajaran MK3-A milik kelompok 5 - XII PPLG 1, untuk praktikum pembuatan RESTful web API CRUD operations melalui Express.js menggunakan ORM Sequelize.",
  "devDependencies": {
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.5"
  }
}
